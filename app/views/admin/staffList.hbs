<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
          Staff List
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-user-circle"></i> Staff Management
            </li>
            <li class="active">
              <a href="#"><i class="fa fa-list"></i> Staff List</a
            </li>
        </ol>
        <div class="col-lg-12">
            <div class="table-responsive">
                <table id="staffTable" class="table table-striped table-bordered">
                    <thead>
                        <tr >
                            <th>UserID</th>
                            <th>Email</th>
                            <th>Name</th>
                            <th>Phone</th>
                            <th>State</th>
                        </tr>
                    </thead>
                    <tbody>
                      {{#each staffs}}
                        <tr class="table-row" id = "row" >
                          <td id ='i' title = "{{this.userid}}" class="row">
                            {{this.userid}}
                          </td>
                            <td id ='e' title = "{{this.email}}" class="row">
                              {{this.email}}
                            </td>
                            <td id = 'n' title = "{{this.name}}" class="row">
                                {{this.name}}
                            </td>
                            <td id = 'p' title = "{{this.phone}}" class="row">
                                {{this.phone}}
                            </td>
                            <td id = 's' title = "{{this.state}}" class="row">
                                {{this.state}}
                            </td>
                        </tr>
                      {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<!-- /.row -->
<script src="js/staffList.js"></script>
